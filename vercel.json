{
  "version": 2,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.js" },
    { "source": "/book/(.*)", "destination": "/api/index.js" },
    { "source": "/series/(.*)", "destination": "/api/index.js" },
    { "source": "/release/(.*)", "destination": "/api/index.js" },
    { "source": "/reader/(.*)", "destination": "/api/index.js" },
    { "source": "/proxy", "destination": "/api/index.js" }
  ]
}
